{
  "name": "davidbowie",
  "version": "0.1.0",
  "description": "Manage change logs and release notes for your software project.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "change",
    "changelog",
    "changes",
    "change-log",
    "release-notes",
    "releases",
    "release notes"
  ],
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:mearns/davidbowie.git"
  },
  "author": "Brian Mearns <mearns.b@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mearns/davidbowie/issues"
  },
  "homepage": "https://github.com/mearns/davidbowie/",
  "engines": {
    "node": ">=8 <=13"
  },
  "scripts": {
    "clean": "rimraf dist/ out/ build/ coverage/ .nyc_output/",
    "pretty": "pretty-quick --staged",
    "pretty-all": "prettier --write ./index.ts ./test/**/*.js ./*.json ./*.yaml ./*.yml",
    "test": "nyc npm run test-only",
    "test-only": "mocha -r ts-node/register ./test/**/*.spec.*",
    "verify": "npm run check && npm run test",
    "check:readme": "remark --no-stdout README.md",
    "check:code": "eslint ./ test/ --ext .js --ext .ts --format codeframe",
    "check": "npm run check:code && npm run check:readme",
    "compile:schema": "mkdir -p dist/ && typescript-json-schema --required --strictNullChecks tsconfig.json ChangeLog --out dist/schema.json && echo 'module.exports = require(\"./schema.json\");' > dist/schema.js && rm -f dist/schema.js.map",
    "compile:tsc": "tsc",
    "compile": "npm run compile:tsc && npm run compile:schema",
    "build": "npm run clean && npm run compile && npm run verify",
    "docs": "typedoc --out out/docs --mode file --includeVersion --exclude \"+(node_modules|dist|out)/**/*\" --includeDeclarations src",
    "prepublishOnly": "rimraf node_modules && npm install && npm audit && npm run build && npm run docs"
  },
  "nyc": {
    "instrument": true,
    "sourceMap": true,
    "all": true,
    "include": [
      "**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/",
      "**/*.d.ts",
      "node_modules/",
      "dist/"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/js-yaml": "^3.12.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "remark-cli": "^7.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.4.0",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.10",
    "typescript": "^3.7.5",
    "typescript-json-schema": "^0.42.0"
  },
  "dependencies": {
    "@types/semver": "^7.1.0",
    "js-yaml": "^3.13.1",
    "jsonschema": "^1.2.5",
    "semver": "^7.1.3"
  }
}
